Host *
	User ubuntu
	IdentityFile id_rsa.txt
	StrictHostKeyChecking no
	PasswordAuthentication no

Host bastionNSO
	HostName 45.114.123.7
	ForwardAgent yes
	ControlMaster auto 
	ControlPath ~/.ssh/ansible-%r@%h:%p
	ControlPersist yes
	
Host HAproxy
	HostName 45.114.123.149
	ForwardAgent yes
	ControlMaster auto 
	ProxyCommand ssh -W %h:%p bastionNSO

